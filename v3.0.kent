set int a = 100 at 0x100

looper(5) {
    a = a + 1
    looper(10) {
        a = a + 2
    }
    a = a + 1
    looper(10) {
        a = a + 1
        looper(10) {
            a = a + 1
        }
    }
    a = a + 2
}

print "a is " + a

set int b = 200 at 0x200

